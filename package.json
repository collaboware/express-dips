{
  "name": "express-ips",
  "version": "0.1.0",
  "description": "",
  "main": "lib/api/index.js",
  "scripts": {
    "postinstall": "yarn generate:shex",
    "generate:shex": "shex-codegen generate",
    "watch:shex": "shex-codegen watch",
    "build:app": "webpack --mode development",
    "build:api": "rimraf lib && tsoa spec-and-routes && openapi --input ./src/generated/swagger.json --output src/generated/openapi --name IPSOpenApi && tsc --resolveJsonModule",
    "build": "yarn build:api && yarn build:app",
    "start": "node lib/api/index.js",
    "develop:api": "nodemon --ext .ts,.tsx --watch src --ignore \"src/generated\" --exec \"yarn build && yarn start\"",
    "develop": "concurrently --kill-others \"yarn develop:api\" \"yarn watch:shex\"",
    "typeorm": "typeorm-ts-node-esm -d ./src/datasource.ts",
    "migrate": "yarn typeorm migration:run",
    "makemigrations": "yarn typeorm migration:generate ./src/migrations/ips",
    "generate:docs": "typedoc ./src/index.ts --name shex-methods --readme ./README.md",
    "predeploy": "yarn generate:docs",
    "prepare": "yarn husky install",
    "deploy": "gh-pages -d docs",
    "test": "yarn build && jest",
    "lint": "eslint --ext .ts src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts}": "yarn lint --cache"
  },
  "keywords": [
    "RDF",
    "ShEx"
  ],
  "author": "Collaboware UG",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "26.0.24",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "concurrently": "^7.1.0",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "3.4.1",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "jest": "26.6.3",
    "lint-staged": "11.2.6",
    "nodemon": "2.0.15",
    "openapi-typescript-codegen": "^0.21.0",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typedoc": "0.22.10",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@inrupt/solid-client-authn-node": "^1.11.7",
    "@types/cookies": "^0.7.7",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.3",
    "js-convert-case": "^4.2.0",
    "pg": "^8.7.3",
    "rdflib": "^2.2.19",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "shex-codegen": "^0.3.27",
    "shex-methods": "^0.1.21",
    "swagger-ui-express": "^4.3.0",
    "tsoa": "^3.14.1",
    "typeorm": "^0.3.4",
    "uuid": "^8.3.2"
  }
}
